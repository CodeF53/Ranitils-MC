{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${displayName}",
  "description": "A small utility mod for Ran (Not to be confused with Ran's utility library \"Ranitils\")\n\n§6Special thanks to these people:§e\nCodeF53 (For helping me make mod)\nplusls (For creating OMMC)\nLuna (For creating Elytra Recast)§r§7\n",
  "authors": [
    "Ran", "CodeF53"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "${license}",
  "icon": "assets/${id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "io.github.ran.minecraft.ranitils.fabric.RanitilsMCFabric"
    ],
